#:import MDLabel kivymd.uix.label.MDLabel
#:import MDButton kivymd.uix.button.MDButton

ScreenManager:
    id: screen_manager
    MainScreen:
    FifthFScreen:
    SixthFScreen:
    SeventhFScreen:
    EighthFScreen:
    NinthFScreen:
    TenthFScreen:
    EleventhFScreen:
    Reader:
