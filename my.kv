                    theme_bg_color: "Custom"
                    md_bg_color: 0.24, 0.71, 0.48, 1

                MDButton:
                    radius: dp(0)
                    size_hint: (None, None)
                    height: dp(40)
                    width: dp(70)
                    pos_hint: {'top': 1}

                    style: 'filled'
                    theme_bg_color: "Custom"
                    md_bg_color: 0.16, 0.48, 0.32, 1

                    on_release:
                        app.next_page()
                                
                    MDButtonText:
                        text: 'След. стр.'
                        theme_text_color: "Custom"
                        text_color: 'white'

                MDButton:
                    radius: dp(0)
                    size_hint: (None, None)
                    height: dp(40)
                    width: dp(70)
                    pos_hint: {'top': 1}

                    style: 'filled'
                    theme_bg_color: "Custom"
                    md_bg_color: 0.16, 0.48, 0.32, 1

                    on_release:
                        app.change_screen('main')
                                
                    MDButtonText:
                        text: 'Назад'
                        theme_text_color: "Custom"
                        text_color: 'white'

        ScrollableLabel:
            id: scrollable_label

            MDLabel:
                id: work_label

                size_hint: (None, None)
                height: self.texture_size[1]
                width: root.width

                theme_bg_color: "Custom"
                md_bg_color: 0.08, 0.25, 0.16, 1

                text: root.work_text
                text_size: self.width, None
                theme_text_color: "Custom"
                text_color: 'white'
                halign: 'left'
                valign: 'top'
                padding: dp(10), dp(10)

<Config>:
    name: 'config'

    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color: 0.08, 0.25, 0.16, 1

        MDBoxLayout:
            size_hint_y: None
            height: dp(40)

            MDButton:
                radius: dp(0)
                size_hint: None, None
                height: dp(40)
                width: dp(80)
                pos_hint: {'top': 1}
                theme_bg_color: "Custom"
                md_bg_color: 0.16, 0.48, 0.32, 1

                on_release:
                    app.change_screen('main')

                MDButtonText:
                    theme_text_color: "Custom"
                    text_color: 'white'
                    text_size: self.size
                    halign: 'center'
                    valign: 'center'
                    text: 'Назад'

            MDLabel:
                theme_bg_color: "Custom"
                md_bg_color: 0.24, 0.71, 0.48, 1
