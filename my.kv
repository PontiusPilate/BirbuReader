<CommonHeader>:
    AnchorLayout:
        anchor_y: 'top'
        size_hint_y: None
        height: dp(40)

        BoxLayout:
            
            MDButton:
                radius: dp(0)
                size_hint: (None, None)
                height: dp(40)
                width: dp(100)

                style: 'filled'
                theme_bg_color: "Custom"
                md_bg_color: 0.16, 0.48, 0.32, 1

                on_release:
                    app.change_screen('config')
                        
                MDButtonIcon:
                    icon: 'cog'
                    pos_hint: {"center_x": .5, "center_y": .5}

            MDLabel:
                id: header_label

                text: root.header_text
                halign: 'center'
                text_color: 'white'
                style: 'filled'
                theme_bg_color: "Custom"
                md_bg_color: 0.24, 0.71, 0.48, 1

            MDButton:
                radius: dp(0)
                size_hint: (None, None)
                height: dp(40)
                width: dp(100)
                pos_hint: {'top': 1}

                style: 'filled'
                theme_bg_color: "Custom"
                md_bg_color: 0.16, 0.48, 0.32, 1

                on_release:
                    app.change_screen('main')
                        
                MDButtonText:
                    text: 'Назад'
                    theme_text_color: "Custom"
                    text_color: 'white'

<MainScreen>:
